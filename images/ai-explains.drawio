        <mxCell id="LVXfSgy_7bovKTVvZtUl-32" value="&lt;span style=&quot;line-height: 107%;&quot;&gt;&lt;a style=&quot;&quot; href=&quot;https://medium.com/@simon_attard/leveraging-large-language-models-in-your-software-applications-9ea520fb2f34&quot;&gt;&lt;font style=&quot;font-size: 11px;&quot;&gt;Leveraging Large Language Models in your Software Applications | by Simon Attard | Jun, 2023 | Medium&lt;/font&gt;&lt;/a&gt;&lt;/span&gt;" style="text;whiteSpace=wrap;html=1;" parent="1" vertex="1">
          <mxGeometry x="20" y="1120" width="270" height="30" as="geometry" />
        </mxCell>
        <mxCell id="LVXfSgy_7bovKTVvZtUl-34" value="Response" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontStyle=1" parent="1" vertex="1">
